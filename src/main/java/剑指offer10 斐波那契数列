public class 剑指offer10 {
    public static void main(String[] args) {
        int n = 5;
        System.out.println(fib(n));
    }
    public static int fib(int n) {
        if(n < 2){
            return n;
        }
        int p = 0, q = 0, r = 1;
        for(int i = 2; i <= n; i++){
            p = q;
            q = r;
            r = (p + q) % 1000000007;
        }
        return r;
    }
}
